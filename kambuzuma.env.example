# Kambuzuma Biological Quantum Computing System Environment Variables
# Copy this file to .env and configure your local environment

# Application Configuration
APP_NAME="Kambuzuma Biological Quantum Computing System"
APP_VERSION="1.0.0"
APP_ENV="development"
APP_DEBUG=true
APP_LOG_LEVEL="debug"

# Rust Configuration
RUST_LOG="kambuzuma=debug,biological_validation=trace,quantum=debug"
RUST_BACKTRACE=1
RUST_MIN_STACK=16777216

# Biological System Parameters
BIOLOGICAL_TEMPERATURE=310.15  # Kelvin (37°C)
BIOLOGICAL_PH=7.4
BIOLOGICAL_IONIC_STRENGTH=0.15  # M (physiological)
BIOLOGICAL_MEMBRANE_POTENTIAL=-70.0  # mV
BIOLOGICAL_ATP_CONCENTRATION=5.0  # mM
BIOLOGICAL_VALIDATION_STRICT=true

# Quantum Computing Parameters
QUANTUM_COHERENCE_THRESHOLD=0.95
QUANTUM_DECOHERENCE_RATE=1e-6  # s^-1
QUANTUM_TUNNELING_BARRIER_HEIGHT=0.25  # eV
QUANTUM_MEMBRANE_THICKNESS=5e-9  # m (5 nm)
QUANTUM_ENERGY_SCALE=4.14e-15  # eV⋅s (reduced Planck constant)
QUANTUM_GATE_FIDELITY=0.999

# Neural Processing Parameters
NEURAL_FIRING_THRESHOLD=-55.0  # mV
NEURAL_RESTING_POTENTIAL=-70.0  # mV
NEURAL_REFRACTORY_PERIOD=2e-3  # s (2 ms)
NEURAL_SYNAPTIC_DELAY=1e-3  # s (1 ms)
NEURAL_DENDRITIC_INTEGRATION_TIME=10e-3  # s (10 ms)
NEURAL_AXONAL_CONDUCTION_VELOCITY=120.0  # m/s

# Membrane Dynamics Parameters
MEMBRANE_CAPACITANCE=1e-6  # F/m^2 (1 µF/cm^2)
MEMBRANE_RESISTANCE=1e6  # Ω⋅m^2
MEMBRANE_THICKNESS=5e-9  # m (5 nm)
MEMBRANE_DIELECTRIC_CONSTANT=2.0
MEMBRANE_SURFACE_CHARGE_DENSITY=0.05  # C/m^2

# Ion Channel Parameters
ION_CHANNEL_CONDUCTANCE=20e-12  # S (20 pS)
ION_CHANNEL_SELECTIVITY_FILTER_LENGTH=1e-9  # m (1 nm)
ION_CHANNEL_BINDING_AFFINITY=1e-6  # M (1 µM)
ION_CHANNEL_OPENING_RATE=1e3  # s^-1 (1 kHz)
ION_CHANNEL_CLOSING_RATE=1e2  # s^-1 (100 Hz)

# Performance and Optimization Parameters
COMPUTATION_THREADS=8
MEMORY_LIMIT=8589934592  # 8 GB in bytes
CACHE_SIZE=134217728  # 128 MB in bytes
OPTIMIZATION_LEVEL=3
SIMD_ENABLED=true
VECTORIZATION_ENABLED=true
PARALLEL_PROCESSING_ENABLED=true

# Monitoring and Logging
LOG_FILE="/tmp/kambuzuma.log"
LOG_ROTATION_SIZE=10485760  # 10 MB
LOG_RETENTION_DAYS=30
METRICS_ENABLED=true
METRICS_PORT=9090
HEALTH_CHECK_INTERVAL=60  # seconds

# Development and Testing
TEST_MODE=false
BENCHMARK_MODE=false
PROFILING_ENABLED=false
DEBUG_ASSERTIONS=true
MEMORY_DEBUGGING=false

# Hardware Configuration
CPU_AFFINITY=""  # Empty for automatic
NUMA_POLICY="interleave"
MEMORY_ALLOCATION_STRATEGY="jemalloc"
THREAD_STACK_SIZE=8388608  # 8 MB

# Scientific Computing Libraries
OPENBLAS_NUM_THREADS=4
OMP_NUM_THREADS=4
MKL_NUM_THREADS=4
LAPACK_NUM_THREADS=4
FFTW_NUM_THREADS=4

# Development Tools
CARGO_BUILD_JOBS=4
CARGO_NET_RETRY=3
CARGO_NET_GIT_FETCH_WITH_CLI=true
CARGO_HTTP_TIMEOUT=30
CARGO_HTTP_LOW_SPEED_LIMIT=10
CARGO_HTTP_MULTIPLEXING=true 